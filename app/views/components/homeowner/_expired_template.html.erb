<style data-test="expired-style">

  <%-# --- Internal Styling --- -%>

  .branded-body {
    <%= Brand.branded_body_default %>
  }

  .branded-header {
    <%= Brand.branded_header_default %>
  }

  .branded-primary {
    <%= Brand.branded_primary_default %>
  }

  .branded-text {
    <%= Brand.branded_text_default %>
  }

  .branded-primary-text {
    <%= Brand.branded_text_default %>
  }

  .branded-content {
    <%= Brand.branded_content_default %>
  }

  .branded-input input:-webkit-autofill {
    <%= Brand.branded_autofill_default %>
  }

  .branded-border {
    <%= Brand.branded_border_default %>
  }

  .branded-input input[type="email"] {
    <%= Brand.branded_border_default %>
  }

  .branded-input input[type="password"] {
    <%= Brand.branded_border_default %>
  }

  .branded-btn {
    <%= Brand.branded_static_background_default %>
    <%= Brand.branded_static_text_default %>
    <%= Brand.branded_static_border_default %>
  }

  .branded-btn:hover {
    <%= Brand.branded_hover_background_default %>
    <%= Brand.branded_hover_text_default %>
    <%= Brand.branded_hover_border_default %>
  }

  .branded-titles h2.top-title {
     <%= Brand.branded_top_header_default %>
  }

  .branded-titles h2.second-title {
    <%= Brand.branded_second_header_default %>
  }

  .branded-primary-text {
    <%= Brand.branded_primary_text_default %>
  }

  .branded-primary-text label.checkbox {
    <%= Brand.branded_input_text_default %>
  }


  <%-# --- Banner --- -%>

  .branded-hero {
    background-image: url("/assets/expiry_banner.png");
  }


  <%-# --- External Styling --- -%>

  .branded-left-box {
    <%= Brand.branded_left_box_default %>
  }

  .branded-right-box {
    <%= Brand.branded_right_box_default %>
  }

  .branded-external-text, .branded-external-text a, .branded-external-text label {
    <%= Brand.branded_external_text_default %>
  }

  .branded-left-text, .branded-left-text h2 {
    <%= Brand.branded_left_text_default %>
  }

  .branded-heading-two {
    <%= Brand.branded_second_heading_default %>
  }


  <%-# --- Submit / Login Button --- -%>

  .login .branded-submit, .invitation .branded-submit {
    <%= Brand.branded_static_background_default %>
  }

  .login .branded-submit:hover, .invitation .branded-submit:hover {
    <%= Brand.branded_static_text_default %>
    <%= Brand.branded_static_border_default %>
  }

  .login .branded-submit, .invitation .branded-submit {
    <%= Brand.branded_login_text_default %>
    <%= Brand.branded_login_border_default %>
  }

  .login .branded-submit:hover, .invitation .branded-submit:hover {
    <%= Brand.branded_static_background_default %>
  }


  <%-# --- Input Fields --- -%>

  .branded-content-box input {
    <%= Brand.branded_input_background_default %>
    <%= Brand.branded_input_text_default %>
  }

  .branded-content-box input:-webkit-autofill {
    <%= Brand.branded_autofill_default %>
    <%= Brand.branded_autofill_text_default %>
  }

  .branded-content-box .branded-checkbox {
    <%= Brand.branded_input_background_default %>
    <%= Brand.branded_checkbox_untick_default %>
  }

  .branded-content-box input[type=checkbox]:checked + i:before {
    <%= Brand.branded_checkbox_tick_default %>
  }

  .branded-content-box input {
    <%= Brand.branded_input_border_default %>
  }

  .branded-content-box input:focus {
    <%= Brand.branded_input_border_default %>
  }

  .branded-content-box .branded-checkbox {
    <%= Brand.branded_input_border_default %>
  }

</style>