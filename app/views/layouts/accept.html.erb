<%= render partial: 'layouts/homeowner_head', locals: { title: "", brand: @brand, plot: @plot } %>
<body class="accept-view">
  <% if @brand&.branded_login_image %>
    <div class="login-background" style="background-image: url('<%= image_path(@brand.branded_login_image) %>')" >
  <% else %>
    <div class="default-login-background">
  <% end %>
  <%= yield %>
  </div>
</body>
