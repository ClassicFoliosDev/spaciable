<% breadcrumb :document_new, @parent %>

<div class="section-header">
  <div class="section-title">
    <% if @document&.partially_expired? && !(current_user.cf_admin?) %>
      <div class="expired"><span class="partial-expired-text"><%= t(".expired", parent: @parent) %></span></div>
    <% end %>
    <%= t(".title") %>
  </div>
</div>

<%= render 'form', parent: @parent, document: @document, redirect_path: @redirect_path %>
