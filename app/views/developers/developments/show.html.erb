<% breadcrumb :development, @development %>

<div class="development">

  <%= render '/developments/development', development: @development %>
  <%= render 'components/tabs', tabs: development_tabs(@development, @active_tab), parent: @developer %>
  <%= render_collection @collection, parent: @development %>

  <div class="form-actions-footer-container">
    <div class="form-actions-footer">
      <%= link_to t(".back"), [@development.developer, active_tab: :developments], class: "btn" %>
    </div>
  </div>

</div>
