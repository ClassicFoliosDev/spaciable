<div class="row card-container">
  <!--MY HOME-->
  <div class="card row-item half my-home" style="background-image: url('http://www.claudehooper.co.uk/wp-content/uploads/2013/08/Gerrards-Cross-Lounge.jpg')">
    <div class="card-title">
      <h3 class="title">
        <%= t('homeowner.dashboard.cards.my_home.title') %>
      </h3>
    </div>
    <div class="card-content">
      <address class="my-home-address">
        <span>34 Baird Street</span><br/>
        <span>Upper Caldecote</span><br/>
        <span>Biggleswade</span><br/>
        <span>Surrey</span><br/>
        <span>United Kingdom</span><br/>
        <span>GU15 1AF</span>
      </address>
      <a class="btn btn-primary" href=""><%= t('homeowner.dashboard.cards.my_home.view_more') %></a>
    </div>
  </div>
  <!--LIBRARY-->
  <div class="card row-item half my-library">
    <div class="card-title">
      <h2 class="title"><%= t('homeowner.dashboard.cards.library.title') %></h2>
      <a href="" class="btn btn-primary"><%= t('homeowner.dashboard.cards.library.view_more') %></a>
    </div>

    <div class="card-content">
      <div class="action action-left">
        <i class="fa fa-chevron-left"></i>
      </div>
      <ul class="documents">
        <li class="document">
          <img src="https://images-na.ssl-images-amazon.com/images/I/51KR3Gx1ASL._SX258_BO1,204,203,200_.jpg">
          <p>Dishwasher Manual</p>
        </li>
        <li class="document">
          <img src="https://s-media-cache-ak0.pinimg.com/564x/61/d8/0a/61d80a2a68e08673154798f4515b7a62.jpg">
          <p>First Aid Checklist</p>
        </li>
        <li class="document">
          <img src="https://www.cdc.gov/niosh/docs/2009-113/images/cover1.jpg">
          <p>Electricity Safety</p>
        </li>
        <li class="document">
          <img src="http://library.plumbase.co.uk/flipbooks/RE/ravlitls80tn_25921_t/ravlitls80tn_25921_t.jpg">
          <p>Boiler Manual</p>
        </li>
        <li class="document">
          <img src="https://data2.manualslib.com/big_thumbs/38/3800/379980_ffh17f7hw__166_cu_ft_frost_free_upright_freezer_product.png">
          <p>Freezer Manual</p>
        </li>
      </ul>
      <div class="action action-right">
        <i class="fa fa-chevron-right"></i>
      </div>
    </div>
  </div>
</div>
