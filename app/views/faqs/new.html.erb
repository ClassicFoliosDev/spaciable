<% breadcrumb :faq_add, @parent %>

<div class="section-header">
  <div class="section-title">
    <% if @parent.partially_expired? && !(current_user.cf_admin?) %>
      <div class="expired"><span class="partial-expired-text"><%= t(".expired", parent: @parent) %></span></div>
    <% end %>
    <%= fa_icon "question-circle" %>
    <%= t(".title", parent: @parent) %>
  </div>
</div>

<%= render 'form', faq: @faq, parent: @parent , :locals => {:country => @parent.country} %>
