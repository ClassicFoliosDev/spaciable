<%= render "components/homeowner/library_hero", active_category: @category, categories: @categories %>

<div>
  <%= render "components/homeowner/sub_header", active_tab: "library" %>
</div>

<div class="main-container">
  <div class="videos">
    <% @videos.each do | video | %>
      <div class="video branded-text">
        <iframe height="245" width="420" src=<%= video.link %> frameborder="0"></iframe>
        <h5><%= video.title %></h5>
      </div>
    <% end %>
  </div>
</div>
